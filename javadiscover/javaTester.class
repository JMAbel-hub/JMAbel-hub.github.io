


public class javaTester {
    public static void main(String[] args) {
        int x = 5;
        int y = 10;
        // Both accomplish the same thing but with different approaches
        paramMethod(x, y);
        System.out.println(noParamMethod());
        
    }
    
    // Syntax of Method Creation
    modifier returnType nameOfMethod (Parameter List) {
        // method body
    }

    public static void paramMethod(int x, int y){
        // adds the two parameters and prints them to console
        int sum = x + y;
        System.out.println(sum);
        // returns void
    }

    public static int noParamMethod(){
        // creates two variables within method
        int x = 5;
        int y = 10;
        // returns an int 
        return x + y;
    }
}